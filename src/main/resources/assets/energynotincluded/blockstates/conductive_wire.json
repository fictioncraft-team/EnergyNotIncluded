{
  "multipart": [
    {
      "apply": {
        "model": "oniutils:block/wires/conductive_wire/main"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "oniutils:block/wires/conductive_wire/side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "oniutils:block/wires/conductive_wire/side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "oniutils:block/wires/conductive_wire/side",
        "y": 180
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "oniutils:block/wires/conductive_wire/side",
        "y": 270
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "oniutils:block/wires/conductive_wire/down",
        "x": 180
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "oniutils:block/wires/conductive_wire/down"
      }
    }
  ]
}